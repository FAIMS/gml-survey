<?xml version="1.0" ?>
<!--@PREPROC:  ./preproc.sh-->
<module>

<!--SUBSTITUTE_THIS-->

<User f="nodata">
  <User>
    <User f="user" l="Control"/>
  </User>
</User>

<Control f="nodata">
  <Control>
    <New_Transect                     t="button" l="Transect"/>
    <New_Site                         t="button" l="Site"/>
    <New_Feature                      t="button" l="Feature"/>
    <New_Objects_IV                   t="button" l="Objects_IV">
      Objects &amp; IV
    </New_Objects_IV>
    <New_Cultural_Management_Comments t="button" l="Cultural_Management_Comments">
      Cultural &amp; Management Comments
    </New_Cultural_Management_Comments>
    <autonum/>
  </Control>
</Control>

<Transect>
  <Transect>
    <gps><!--Start Survey Unit--></gps>
    <Transect_ID f="id notnull autonum"/>
    <timestamp/>
    <Device_ID f="notnull"/>
    <Logger_ID f="notnull"/>
    <Total_Transect_Width_m>Total Transect Width (m)</Total_Transect_Width_m>
    <Number_of_Participants_in_Transect f="notnull"/>
    <Participants>
      <desc>
        Enter the names of assistants and other people walking your survey
        transect.
      </desc>
    </Participants>
    <Landform f="notnull">
      <desc>
        Include definitions of these terms from p.12 of Australian Soil and Land
        Survey Handbook
      </desc>
    </Landform>
    <Slope_Gradient f="notnull">Slope/Gradient</Slope_Gradient>
    <Aspect/>
    <Vegetation_notes f="notnull"/>
    <Ground_surface_visibility f="notnull"/>
    <Ground_surface_exposure f="notnull"/>
    <Ground_surface_notes/>
    <Soil_observations/>
    <Geomorphological_Activity f="notnull"/>
    <Disturbance/>
    <Photo/>
    <Photo_Caption/>
    <Sketch/>
    <Other_Notes/>
    <gps><!--Finish Survey Unit--></gps>
  </Transect>
  <Map>
    <Map t="map"/>
    <Survey_extent f="readonly"/>
  </Map>
</Transect>

<Site>
  <Site>
    <Site_ID_Number f="id notnull autonum"/>
    <Open_Closed_Site>Open/Closed Site</Open_Closed_Site>
    <Site_Aspect_Orientation>Site Aspect/Orientation</Site_Aspect_Orientation>
    <Distance_to_permanent_water_source/>
    <Distance_to_temporary_water_source/>
    <Name_of_nearest_permanent_water_source/>
    <Name_of_nearest_temporary_water/>
    <General_Condition_of_Site f="notnull"/>
  </Site>
  <Open_Site>
    <Total_length_of_visible_site f="notnull"/>
    <Average_width_of_visible_site f="notnull"/>
    <Estimated_area_of_visible_site f="notnull"/>
    <Length_of_assessed_site_area f="notnull"/>
    <Subsurfance_potential t="radio">
      <opts>
        <opt>Yes</opt>
        <opt>No</opt>
      </opts>
    </Subsurfance_potential>
    <Length_of_potential_site_area/>
    <Average_width_of_potential_site_area f="notnull"/>
  </Open_Site>
  <Closed_Site>
    <Shelter_Cave_Formation>
      Shelter/Cave Formation
    </Shelter_Cave_Formation>
    <Rock_Surface_Condition/>
    <Internal_Length f="notnull"/>
    <Internal_Width f="notnull"/>
    <Shelter_Height f="notnull"/>
    <Shelter_Floor_Area f="notnull"/>
  </Closed_Site>
</Site>

<Feature>
  <Feature>
    <Feature_ID f="id autonum"/>
    <Feature_Type/>
    <Feature_Condition/>
  </Feature>
</Feature>

<Objects_IV>
  Objects &amp; IV
  <Artefact>
    <Insitu/>
    <Stratified/>
    <Total_Number_of_Artefacts/>
    <Instance_No f="id">
      Instance No.
    </Instance_No>
    <Artefact_Raw_Material/>
    <Artefact_Type/>
    <Artefact_Description_for_Aboriginal_Stone_Artefacts>
      Artefact Description (for Aboriginal Stone Artefacts)
    </Artefact_Description_for_Aboriginal_Stone_Artefacts>
    <Artefact_Description_for_Historical_Artefacts>
      Artefact Description (for Historical Artefacts)
    </Artefact_Description_for_Historical_Artefacts>
    <Artefact_Colour/>
    <Artefact_Status_Modification>
      Artefact Status/Modification
    </Artefact_Status_Modification>
    <Platform_Surface_for_Aboriginal_stone_artefacts>
      Platform Surface (for Aboriginal stone artefacts)
    </Platform_Surface_for_Aboriginal_stone_artefacts>
    <Platform_Type_for_Aboriginal_stone_artefacts>
      Platform Type (for Aboriginal stone artefacts)
    </Platform_Type_for_Aboriginal_stone_artefacts>
    <Termination_for_Aboriginal_stone_artefacts>
      Termination (for Aboriginal stone artefacts)
    </Termination_for_Aboriginal_stone_artefacts>
    <Cross_Section_for_Aboriginal_artefacts>
      Cross Section (for Aboriginal artefacts)
    </Cross_Section_for_Aboriginal_artefacts>
    <Length_mm>Length (mm)</Length_mm>
    <Width_mm>Width (mm)</Width_mm>
    <Thickness_mm>Thickness (mm)</Thickness_mm>
    <Other_Observations/>
    <Photo/>
    <Photo_Caption/>
  </Artefact>
  <Intangible_Values>
    <Intangible_Value_ID f="autonum"/>
    <Intangible_Value_Type/>
    <Description/>
  </Intangible_Values>
  <Art>
    <Art_ID/>
    <Pigment_Engraved>Pigment/Engraved</Pigment_Engraved>
    <Super-impositioning/>
    <Motif_s>Motif/s</Motif_s>
    <Application_Technique/>
    <Form/>
    <Main_Colour/>
    <Art_Location/>
    <Associated_Story_Significance_Interpretation>
      Associated Story/Significance/Interpretation
    </Associated_Story_Significance_Interpretation>
    <Art_Condition/>
    <Sketch_plan/>
    <Photo/>
    <Other_Observations/>
  </Art>
  <Other_Feature>
    <Other_Feature_ID f="autonum"/>
    <Description/>
    <Chain_of_Custody_Notes_for_human_remains>
      Chain of Custody Notes (for human remains)
    </Chain_of_Custody_Notes_for_human_remains>
    <Sketch_Plan/>
    <Photo/>
  </Other_Feature>
  <Grinding_Grove>
    <Grinding_Grove_ID f="autonum"/>
    <Type/>
    <Profile_Shape/>
    <Seed_species_present/>
    <Groove_function/>
    <Dimensions_-_length_of_smallest_part_mm>
      Dimensions - length of smallest part (mm)
    </Dimensions_-_length_of_smallest_part_mm>
    <Dimensions_-_width_of_smallest_part_mm>
      Dimensions - width of smallest part (mm)
    </Dimensions_-_width_of_smallest_part_mm>
    <Dimensions_-_depth_of_smallest_part_mm>
      Dimensions - depth of smallest part (mm)
    </Dimensions_-_depth_of_smallest_part_mm>
    <Dimensions_-_length_of_largest_part_mm>
      Dimensions - length of largest part (mm)
    </Dimensions_-_length_of_largest_part_mm>
    <Dimensions_-_width_of_largest_part_mm>
      Dimensions - width of largest part (mm)
    </Dimensions_-_width_of_largest_part_mm>
    <Dimensions_-_depth_of_largest_part_mm>
      Dimensions - depth of largest part (mm)
    </Dimensions_-_depth_of_largest_part_mm>
    <Groove_count/>
    <Cluster_count/>
    <Sketch_Plan/>
    <Photo/>
  </Grinding_Grove>
  <Shell>
    <Shell_ID f="autonum"/>
    <Distribution/>
    <Distance_to_high_water_mark_metres>
      Distance to high water mark (metres)
    </Distance_to_high_water_mark_metres>
    <Species_present/>
    <Dominant_Species/>
    <Approximate_percentage_of_dominant_species_as_part_of_assemblage/>
    <Sketch_Plan/>
    <Photo/>
  </Shell>
  <Hearth_Ground_Oven>
    Hearth/Ground Oven
    <Hearth_ID/>
    <Shape/>
    <Length/>
    <Width/>
    <Estimated_Depth/>
    <Components/>
    <Descriptive_Notes/>
    <Sketch_Plan/>
    <Photo/>
  </Hearth_Ground_Oven>
  <Potential_Archaeological_Deposit>
    <Arch_Dep_ID f="id autonum"/>
    <Insitu/>
    <Stratified/>
    <Deposit_Type/>
    <Surface_Artefacts/>
  </Potential_Archaeological_Deposit>
  <Culturally_Modified_Tree>
    <CM_Tree_ID f="id autonum"/>
    <Type/>
    <Tree_Species/>
    <Number_of_Scars/>
    <Number_of_Carved_Panels/>
    <Living_Status/>
    <Tree_Status/>
    <Damage/>
    <Regrowth/>
    <Is_a_tree_health_assessment_required/>
    <Length_of_Scar_mm>Length of Scar (mm)</Length_of_Scar_mm>
    <Width_of_Scar_mm>Width of Scar (mm)</Width_of_Scar_mm>
    <Depth_of_Scar_mm>Depth of Scar (mm)</Depth_of_Scar_mm>
    <Height_Above_Ground_mm>Height Above Ground (mm)</Height_Above_Ground_mm>
    <Shape/>
    <Orientation/>
    <Carving_type/>
    <Axe_Marks/>
  </Culturally_Modified_Tree>
</Objects_IV>

<Cultural_Management_Comments>
  Cultural &amp; Management Comments
  <C_M_Comments>
    C &amp; M Comments
    <Comment_ID f="id autonum"/>
    <Aboriginal_community_interpretation/>
    <Aboriginal_community_management_recommendations/>
    <Other_management_recommendations/>
  </C_M_Comments>
</Cultural_Management_Comments>

<logic><![CDATA[
]]></logic>

</module>
